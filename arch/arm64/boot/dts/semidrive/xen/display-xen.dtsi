				parallel3_out: port@1 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <1>;
					parallel_out_timing3: endpoint@0 {
						reg = <0>;
						remote-endpoint = <&parall_panel3_in>;
					};
				};
			};
		};

		lvds0: lvds0@30660000 {
			compatible = "semidrive, kunlun-display-lvds";
			reg = <0x0 0x30c40000 0x0 0x50000>;
			lvds-select = <1>;
			dual-mode = <0>;
			lvds-combine = <0>;
			map-format = <1>;
			pixel-format = <1>;

			status = "disabled";

			ports {
				#address-cells = <1>;
				#size-cells = <0>;
				lvds0_in: port@0 {
					lvds_in_crtc0: endpoint@0 {
						remote-endpoint = <&crtc0_out_interface>;
					};
				};

				lvds0_out: port@1 {
					#address-cells = <1>;
