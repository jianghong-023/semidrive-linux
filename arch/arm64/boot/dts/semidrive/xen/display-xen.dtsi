						reg = <0>;
						remote-endpoint = <&parall_panel0_in>;
					};
				};
			};
		};

		lvds1: lvds1@30670000 {
			compatible = "semidrive, kunlun-display-lvds";
			reg = <0x0 0x30c40000 0x0 0x50000>;
			lvds-select = <2>;
			dual-mode = <0>;
			lvds-combine = <0>;
			map-format = <1>;
			pixel-format = <1>;

			status = "disabled";

			ports {
				#address-cells = <1>;
				#size-cells = <0>;
				lvds1_in: port@0 {
					lvds_in_crtc1: endpoint@0 {
						remote-endpoint = <&crtc1_out_interface>;
